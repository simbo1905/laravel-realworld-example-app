#!/bin/bash
echo "Running default /usr/libexec/s2i/assemble"

/usr/libexec/s2i/assemble
rc=$?

if [ $rc -eq 0 ]; then
    echo "After successful assembling copying files"
    # setup the nginx config
    cp /opt/app-root/src/etc/nginx/container.conf /opt/app-root/etc/conf.d/nginx/servers/default


else
    echo "After failed assembling"
fi

exit $rc
